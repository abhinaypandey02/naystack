{
  "name": "naystack",
  "version": "1.4.6",
  "description": "A stack built with Next + GraphQL + S3 + Auth",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=16384' tsup"
  },
  "exports": {
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.esm.js",
      "require": "./dist/auth/index.cjs.js"
    },
    "./socials": {
      "types": "./dist/socials/index.d.ts",
      "import": "./dist/socials/index.esm.js",
      "require": "./dist/socials/index.cjs.js"
    },
    "./graphql": {
      "types": "./dist/graphql/index.d.ts",
      "import": "./dist/graphql/index.esm.js",
      "require": "./dist/graphql/index.cjs.js"
    },
    "./graphql/client": {
      "types": "./dist/graphql/client.d.ts",
      "import": "./dist/graphql/client.esm.js",
      "require": "./dist/graphql/client.cjs.js"
    },
    "./auth/email/client": {
      "types": "./dist/auth/email/client.d.ts",
      "import": "./dist/auth/email/client.esm.js",
      "require": "./dist/auth/email/client.cjs.js"
    },
    "./auth/instagram/client": {
      "types": "./dist/auth/instagram/client.d.ts",
      "import": "./dist/auth/instagram/client.esm.js",
      "require": "./dist/auth/instagram/client.cjs.js"
    },
    "./file/client": {
      "types": "./dist/file/client.d.ts",
      "import": "./dist/file/client.esm.js",
      "require": "./dist/file/client.cjs.js"
    },
    "./graphql/server": {
      "types": "./dist/graphql/server.d.ts",
      "import": "./dist/graphql/server.esm.js",
      "require": "./dist/graphql/server.cjs.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.esm.js",
      "require": "./dist/client/index.cjs.js"
    },
    "./file": {
      "types": "./dist/file/index.d.ts",
      "import": "./dist/file/index.esm.js",
      "require": "./dist/file/index.cjs.js"
    }
  },
  "keywords": [
    "typescript",
    "graphql",
    "nextjs",
    "drizzle-orm",
    "nextjs-api",
    "typescript-library",
    "backend",
    "fullstack",
    "api",
    "database",
    "server",
    "graphql-api",
    "nextjs-routes",
    "drizzle",
    "orm"
  ],
  "author": "Abhinay Pandey <abhinaypandey02@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.13.6",
    "@apollo/client-integration-nextjs": "^0.12.1",
    "@apollo/server": "^4.12.0",
    "@as-integrations/next": "^3.2.0",
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@vercel/functions": "^3.1.0",
    "bcryptjs": "^3.0.2",
    "drizzle-orm": "^0.44.5",
    "googleapis": "^160.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^16",
    "react": "^19.2",
    "react-dom": "^19.2",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "2.0.0-rc.2",
    "uuid": "^13.0.0",
    "v4": "^0.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/cloudflare-turnstile": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.5.2",
    "@types/react": "^19.2",
    "@types/react-dom": "^19.2",
    "dotenv": "^17.2.2",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^4.0.0-alpha.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "google-auth-library": "^10.3.0",
    "graphql": "^16.11.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.31.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abhinaypandey02/naystack.git"
  }
}
